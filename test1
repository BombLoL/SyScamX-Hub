local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = " [SyScam X Hub] ",
    LoadingTitle = "Loading [SyScam X]",
    LoadingSubtitle = "Made by: [w3irdo]",
    ConfigurationSaving = {
       Enabled = true,
       FolderName = "SyScam X", -- Create a custom folder for your hub/game
       FileName = "SyScam X File"
    },
    Discord = {
       Enabled = false,
       Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD
       RememberJoins = true -- Set this to false to make them join the discord every time they load it up
    },
    KeySystem = true, -- Set this to true to use our key system
    KeySettings = {
       Title = "SyScam X Hub | Key System",
       Subtitle = "Key System",
       Note = "Use the key: SynapseScams or SyScamX",
       FileName = "k333y", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
       SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
       GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
       Key = {"SynapseScams","SyScamX"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
    }
 })

 local Tab = Window:CreateTab("Games", 15672947501) -- Tabby tab
 local Paragraph = Tab:CreateParagraph({Title = "Welcome to SyScams X!", Content = "This is a joke! Do not take this seriously."})
 local Section = Tab:CreateSection("Scripts")
 local arsenal = Section:CreateButton({
    Name = "Arsenal",
    Callback = function()
      script.Parent.MouseButton1Click:Connect(function()
            loadstring(game:HttpGet('https://raw.githubusercontent.com/BombLoL/SyScamX-Hub/main/HamewareScript'))()
      end)
    end,
 })
 local ps99 = Section:CreateButton({
    Name = "Pet Simulator 99",
    Callback = function()
      script.Parent.MouseButton1Click:Connect(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/REDzHUB/PetSimulator99/main/redz9999.lua"))()
      end)
    end,
 })
 local mm2 = Section:CreateButton({
    Name = "Murder Mystery 2 and Fling/Yeet Gui (FE)",
    Callback = function()
      script.Parent.MouseButton1Click:Connect(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/vwSaraa/LunarHub/main/mm2"))()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/BombLoL/SyScamX-Hub/main/Fling%20Gui"))()
      end)
    end,
 })
 local bladeballs = Section:CreateButton({
    Name = "Blade Ball",
    Callback = function()
      script.Parent.MouseButton1Click:Connect(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/3345-c-a-t-s-u-s/SourceLua/main/Blade_Ball.lua"))()
      end)
    end,
 })
 local bloxfruit = Section:CreateButton({
    Name = "Blox Fruit",
    Callback = function()
      script.Parent.MouseButton1Click:Connect(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/REDzHUB/BloxFruits/main/redz9999"))()
      end)
    end,
 })
